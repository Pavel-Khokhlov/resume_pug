    "autoprefixer": "10.4.5",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.7",
    "gh-pages": "^3.2.3",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.5.0",
    "install": "^0.13.0",
    "mini-css-extract-plugin": "^2.6.0",
